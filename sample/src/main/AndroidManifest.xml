<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yqritc.recyclerviewflexibledivider.sample">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".AllLinearActivity"
            android:label="@string/menu_all_linear">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PaintActivity"
            android:label="@string/menu_paint" />
        <activity
            android:name=".DrawableActivity"
            android:label="@string/menu_drawable" />
        <activity
            android:name=".ComplexActivity"
            android:label="@string/menu_complex" />
        <activity
            android:name=".SimpleGridActivity"
            android:label="@string/menu_simple_grid" />
        <activity
            android:name=".SimpleActivity"
            android:label="@string/menu_simple" />
    </application>

</manifest>
